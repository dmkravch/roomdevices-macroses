<Extensions>
  <Version>1.7</Version>
  <Panel>
    <Order>1</Order>
    <PanelId>people_count</PanelId>
    <Origin>local</Origin>
    <Type>Home</Type>
    <Icon>Lightbulb</Icon>
    <Name>People Count</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>People Count Action</Name>
      <Row>
        <Name>Activate</Name>
        <Widget>
          <WidgetId>people_count_activate</WidgetId>
          <Type>ToggleButton</Type>
          <Options>size=1</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
  <Panel>
    <Order>2</Order>
    <PanelId>panel_2</PanelId>
    <Origin>local</Origin>
    <Type>Home</Type>
    <Icon>Microphone</Icon>
    <Color>#FF3D67</Color>
    <Name>Ultrasound</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>Ultrasound</Name>
      <Row>
        <Name>Volume</Name>
        <Widget>
          <WidgetId>volume_text</WidgetId>
          <Name>?</Name>
          <Type>Text</Type>
          <Options>size=1;fontSize=normal;align=center</Options>
        </Widget>
        <Widget>
          <WidgetId>volume_slider</WidgetId>
          <Type>Slider</Type>
          <Options>size=3</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
  <Panel>
    <Order>3</Order>
    <PanelId>Hue</PanelId>
    <Origin>local</Origin>
    <Type>Statusbar</Type>
    <Icon>Sliders</Icon>
    <Color>#A866FF</Color>
    <Name>Philips Hue</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>Basic</Name>
      <Row>
        <Name>Switch</Name>
        <Widget>
          <WidgetId>toggle</WidgetId>
          <Type>ToggleButton</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>widget_22</WidgetId>
          <Name/>
          <Type>Text</Type>
          <Options>size=3;fontSize=normal;align=center</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Pick a color</Name>
        <Widget>
          <WidgetId>color_group</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4</Options>
          <ValueSpace>
            <Value>
              <Key>color_red</Key>
              <Name>Red</Name>
            </Value>
            <Value>
              <Key>color_green</Key>
              <Name>Green</Name>
            </Value>
            <Value>
              <Key>color_blue</Key>
              <Name>Blue</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Row>
        <Name>Brightness</Name>
        <Widget>
          <WidgetId>overall_brightness</WidgetId>
          <Type>Slider</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
